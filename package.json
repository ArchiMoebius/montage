{
  "name": "montage",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "npm run lint && electron-webpack dev",
    "compile": "npm run lint && electron-webpack",
    "dist": "npm run lint && npm run compile && ./node_modules/.bin/electron-builder",
    "dist:dir": "npm run lint && npm dist --dir -c.compression=store -c.mac.identity=null",
    "lint": "./node_modules/.bin/eslint --ext .js,.vue -f ./node_modules/eslint-friendly-formatter src"
  },
  "dependencies": {
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-0": "^6.24.1",
    "dexie": "^2.0.2",
    "sharp": "^0.20.1",
    "source-map-support": "^0.5.4",
    "vue": "^2.5.16",
    "vue-electron": "^1.0.6",
    "vue-material": "^1.0.0-beta-8",
    "vue-router": "^3.0.1",
    "vue-template-compiler": "^2.5.16",
    "vuelidate": "^0.6.2",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.3",
    "electron": "1.8.4",
    "electron-builder": "^20.8.1",
    "electron-debug": "^1.5.0",
    "electron-webpack": "^2.0.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-import-resolver-webpack": "^0.9.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-html": "^4.0.3",
    "eslint-plugin-import": "^2.11.0",
    "vue-loader": "^14.2.2",
    "webpack": "^4.4.1"
  },
  "resolutions": {
    "webpack-sources": "1.0.1"
  },
  "build": {
    "asar": false
  }
}
